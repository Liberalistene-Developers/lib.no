<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part>
  <display-name>Event</display-name>
  <description>
    Display Event
  </description>
  <form>
    <mixin name="headerposition" />
    <mixin name="headercolor" />
    <mixin name="ingressinimage" />
    <mixin name="titleinimage" />
    <input name="informationLabel" type="TextLine">
      <label i18n="content_types.event.information.title">Information Title</label>
      <default>Informasjon</default>
    </input>
    <input name="moreInformationLabel" type="TextLine">
      <label i18n="content_types.event.information.title">More Information Title</label>
      <default>Mer Informasjon</default>
    </input>
    <input name="locationLabel" type="TextLine">
      <label i18n="content_types.event.location.title">Place Title</label>
      <default>Sted</default>
    </input>
    <input name="contactLabel" type="TextLine">
      <label i18n="content_types.event.contact.title">Contact Title</label>
      <default>Mer Informasjon</default>
    </input>
    <input name="placeLabel" type="TextLine">
      <label i18n="content_types.event.place.title">Place label</label>
      <default>Sted</default>
    </input>
    <input name="agendaLabel" type="TextLine">
      <label i18n="content_types.event.agenda.title">Agenda Title</label>
      <default>Program</default>
    </input>
    <input name="dateLabel" type="TextLine">
      <label i18n="content_types.event.date.title">Date label</label>
      <default>Dato</default>
    </input>
    <input name="timeLabel" type="TextLine">
      <label i18n="content_types.event.time.title">Date label</label>
      <default>Tid</default>
    </input>
  </form>
</part>
