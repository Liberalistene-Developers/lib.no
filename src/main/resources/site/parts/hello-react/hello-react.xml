<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part>
<display-name>Hello React</display-name>
<description>First example: simple react component where the JSX entry is local to the part: hello-react.jsx</description>
<form>
  <input type="TextLine" name="greetee" > 
    <label i18n="greetee.label">Greetee</label> 
    <default>world</default>
    <help-text i18n="greetee.help-text">Input your greetee.</help-text>  
    <occurrences minimum="0" maximum="1"/>  
    <config/> 
  </input>
</form>
</part>
