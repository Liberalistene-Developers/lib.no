{
  "name": "lib.no",
  "version": "2.0.3",
  "description": "Liberalistene homepage",
  "author": "Benny Thomas",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liberalistene-Developers/lib.no.git"
  },
  "bugs": {
    "url": "https://github.com/Liberalistene-Developers/lib.no/issues"
  },
  "homepage": "https://github.com/Liberalistene-Developers/lib.no#readme",
  "engines": {
    "node": ">= 22.15.1",
    "npm": ">= 10.9.2"
  },
  "engineStrict": false,
  "scripts": {
    "nvm": "nvm use",
    "commit:msg": "commitlint --edit $1",
    "browserSync": "npx browser-sync start --proxy 'localhost:8080' --files build/resources/main/**/* --reload-delay 5000",
    "build": "./gradlew build",
    "build:prod": "./gradlew build -Penv=prod",
    "build:analyze": "ANALYZE=true ./gradlew build",
    "build:assets": "npm run build:assets:js && npm run build:assets:css",
    "build:assets:js": "npx tsup -d build/resources/main/assets || exit 0",
    "build:assets:css": "npx postcss src/assets/tailwind.css -o build/resources/main/assets/styles/tailwind.css",
    "build:react4xp": "npm explore @enonic/react4xp -- npm run build:react4xp",
    "build:xp:resources": "npx tsup -d build/resources/main",
    "check": "concurrently -c auto -g --timings npm:check:types npm:lint npm:lint:css",
    "check:types": "concurrently -g -r --timings npm:check:types:*",
    "check:types:node": "npx tsc --noEmit -p tsconfig.node.json",
    "check:types:react4xp": "npx tsc --noEmit -p tsconfig.react4xp.json",
    "check:types:xp": "npx tsc --noEmit -p tsconfig.xp.nashorn.json",
    "clean": "rimraf build",
    "deploy": "enonic project deploy",
    "deps": "./gradlew --refresh-dependencies",
    "lint": "eslint --cache .",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "lint:css:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "test": "jest --no-cache",
    "test:coverage": "jest --no-cache --coverage",
    "test:watch": "jest --watch",
    "rewatch": "./gradlew clean && enonic project deploy -c",
    "watch": "enonic project deploy -c",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "screenshots": "npx tsx util/capture-screenshots.ts",
    "postinstall": "husky"
  },
  "dependencies": {
    "@enonic/cli": "^3.3.1",
    "@enonic/js-utils": "^1",
    "@enonic/react-components": "^6.0.0",
    "classnames": "^2.3.1",
    "html-react-parser": "^5.2.6",
    "leaflet": "^1.9.4",
    "node-fetch": "^3.2.10",
    "react": "^18",
    "react-dom": "^18",
    "react-leaflet": "^4.2.1",
    "react-slugify": "^5.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@enonic-types/global": "^7",
    "@enonic-types/lib-admin": "^7",
    "@enonic-types/lib-app": "^7",
    "@enonic-types/lib-asset": "^1",
    "@enonic-types/lib-auditlog": "^7",
    "@enonic-types/lib-auth": "^7",
    "@enonic-types/lib-cluster": "^7",
    "@enonic-types/lib-common": "^7",
    "@enonic-types/lib-content": "^7",
    "@enonic-types/lib-context": "^7",
    "@enonic-types/lib-event": "^7",
    "@enonic-types/lib-export": "^7",
    "@enonic-types/lib-grid": "^7",
    "@enonic-types/lib-i18n": "^7",
    "@enonic-types/lib-io": "^7",
    "@enonic-types/lib-mail": "^7",
    "@enonic-types/lib-node": "^7",
    "@enonic-types/lib-portal": "^7",
    "@enonic-types/lib-project": "^7",
    "@enonic-types/lib-react4xp": "^6.0.1",
    "@enonic-types/lib-repo": "^7",
    "@enonic-types/lib-scheduler": "^7",
    "@enonic-types/lib-schema": "^7",
    "@enonic-types/lib-task": "^7",
    "@enonic-types/lib-value": "^7",
    "@enonic-types/lib-vhost": "^7",
    "@enonic-types/lib-websocket": "^7",
    "@enonic/react4xp": "^6.0.2",
    "@eslint/js": "^9.33.0",
    "@item-enonic-types/lib-menu": "^4.2.1",
    "@playwright/test": "^1.56.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-links": "^9.1.13",
    "@storybook/addon-styling-webpack": "^2.0.0",
    "@storybook/react-webpack5": "^9.1.10",
    "@swc/core": "^1",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/css-modules": "^1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.21",
    "browser-sync": "^3",
    "browserslist-config-enonic": "^1.0.8",
    "concurrently": "^9.2.0",
    "css-loader": "^7",
    "esbuild": "^0.25.10",
    "eslint": "^9.33.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-storybook": "^9.1.13",
    "glob": "^11",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.0.5",
    "last-release-git": "^0.0.3",
    "mini-css-extract-plugin": "^2",
    "patch-package": "^8.0.1",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-loader": "^8.2.0",
    "rimraf": "^6",
    "semantic-release": "^24.2.9",
    "sharp": "^0.34.4",
    "storybook": "^9.1.10",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^4.1.15",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tslib": "^2",
    "tsup": "^8",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "typescript-eslint": "^8.40.0",
    "typescript-plugin-css-modules": "^5",
    "webpack": "^5",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "release": {
    "branches": [
      "master"
    ],
    "getLastRelease": "last-release-git",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "sed -i \"s/^version = .*/version = ${nextRelease.version}/\" gradle.properties && ./gradlew clean build -Penv=prod && echo 'RELEASE_BUILT=true' >> $GITHUB_ENV"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json",
            "gradle.properties"
          ]
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "build/libs/Liberalistene.jar",
              "label": "Liberalistene Application (JAR)"
            }
          ]
        }
      ]
    ]
  }
}
