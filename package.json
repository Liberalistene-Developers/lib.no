{
  "name": "lib.no",
  "version": "1.10.2",
  "description": "Liberalistene homepage",
  "author": "Benny Thomas",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liberalistene-Developers/lib.no.git"
  },
  "bugs": {
    "url": "https://github.com/Liberalistene-Developers/lib.no#issues"
  },
  "homepage": "https://github.com/Liberalistene-Developers/lib.no#readme",
  "engines": {
    "node": ">= 22.15.1",
    "npm": ">= 10.9.2"
  },
  "scripts": {
    "nvm": "nvm use",
    "commit:msg": "commitlint --edit $1",
    "browserSync": "npx browser-sync start --proxy 'localhost:8080' --files build/resources/main/**/* --reload-delay 5000",
    "build": "./gradlew build",
    "build:assets": "npx tsup -d build/resources/main/assets || exit 0",
    "build:react4xp": "npm explore @enonic/react4xp -- npm run build:react4xp",
    "build:xp:resources": "npx tsup -d build/resources/main",
    "check": "concurrently -c auto -g --timings npm:check:types npm:lint",
    "check:types": "concurrently -g -r --timings npm:check:types:*",
    "check:types:node": "npx tsc --noEmit -p tsconfig.node.json",
    "check:types:react4xp": "npx tsc --noEmit -p tsconfig.react4xp.json",
    "check:types:xp": "npx tsc --noEmit -p tsconfig.xp.nashorn.json",
    "clean": "rimraf build",
    "deploy": "./gradlew deploy",
    "deps": "./gradlew --refresh-dependencies",
    "lint": "eslint --cache",
    "test": "jest --no-cache",
    "rewatch": "./gradlew clean deploy && ./gradlew -t deploy",
    "watch": "./gradlew -t deploy",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@enonic/js-utils": "^1",
    "@enonic/react-components": "^6.0.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "classnames": "^2.3.1",
    "html-react-parser": "^5.2.6",
    "ini": "^3.0.1",
    "leaflet": "^1.9.4",
    "node-fetch": "^3.2.10",
    "prop-types": "^15.8.1",
    "react": "^18",
    "react-dom": "^18",
    "react-leaflet": "^4.2.1",
    "react-slugify": "^3.0.2",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@enonic-types/global": "^7",
    "@enonic-types/lib-admin": "^7",
    "@enonic-types/lib-app": "^7",
    "@enonic-types/lib-asset": "^1",
    "@enonic-types/lib-auditlog": "^7",
    "@enonic-types/lib-auth": "^7",
    "@enonic-types/lib-cluster": "^7",
    "@enonic-types/lib-common": "^7",
    "@enonic-types/lib-content": "^7",
    "@enonic-types/lib-context": "^7",
    "@enonic-types/lib-event": "^7",
    "@enonic-types/lib-export": "^7",
    "@enonic-types/lib-grid": "^7",
    "@enonic-types/lib-i18n": "^7",
    "@enonic-types/lib-io": "^7",
    "@enonic-types/lib-mail": "^7",
    "@enonic-types/lib-node": "^7",
    "@enonic-types/lib-portal": "^7",
    "@enonic-types/lib-project": "^7",
    "@enonic-types/lib-react4xp": "^6.0.1",
    "@enonic-types/lib-repo": "^7",
    "@enonic-types/lib-scheduler": "^7",
    "@enonic-types/lib-schema": "^7",
    "@enonic-types/lib-task": "^7",
    "@enonic-types/lib-value": "^7",
    "@enonic-types/lib-vhost": "^7",
    "@enonic-types/lib-websocket": "^7",
    "@enonic/react4xp": "^6.0.2",
    "@eslint/js": "^9.33.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-links": "9.1.10",
    "@storybook/addon-styling-webpack": "^2.0.0",
    "@storybook/react-webpack5": "^9.1.10",
    "@swc/core": "^1",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/css-modules": "^1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.21",
    "browser-sync": "^3",
    "browserslist-config-enonic": "^1.0.8",
    "concurrently": "^9.2.0",
    "css-loader": "^7",
    "esbuild": "^0.25.10",
    "eslint": "^9.33.0",
    "eslint-plugin-storybook": "9.1.10",
    "glob": "^8.1.0",
    "husky": "^8.0.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.0.5",
    "last-release-git": "^0.0.3",
    "mini-css-extract-plugin": "^2",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "rimraf": "^6",
    "sass": "^1",
    "sass-loader": "^16",
    "semantic-release": "^24.2.9",
    "storybook": "^9.1.10",
    "tailwindcss": "^4.1.14",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tslib": "^2",
    "tsup": "^8",
    "typescript": "^5",
    "typescript-eslint": "^8.40.0",
    "typescript-plugin-css-modules": "^5",
    "webpack": "^5",
    "webpack-cli": "^5"
  },
  "overrides": {
    "glob-promise": {
      "glob": "^8.1.0"
    }
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-arrow-functions"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "getLastRelease": "last-release-git",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ]
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github"
    ]
  }
}
