{
  "name": "lib.no",
  "version": "0.0.1",
  "description": "Liberalistene homepage",
  "scripts": {
    "rm": "node_modules/rimraf/bin.js build",
    "install:dev": "nsi",
    "install:prod": "npm install",
    "rebuild": "npm run rm; ./gradlew build",
    "deploy": "./gradlew deploy --info",
    "watch": "npm-run-all --parallel watch:xp watch:r4xp",
    "watch:gradle": "./gradlew deploy --info -t",
    "watch:r4xp": "webpack --env.REACT4XP_CONFIG_FILE=build/react4xp_constants.json --config node_modules/react4xp-build-components/webpack.config.js --progress --watch",
    "watch:xp": "babel src/main/resources --out-dir build/resources/main --ignore **/*.jsx --progress --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liberalistene-Developers/lib.no.git"
  },
  "author": "Benny Thomas",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Liberalistene-Developers/lib.no#issues"
  },
  "homepage": "https://github.com/Liberalistene-Developers/lib.no#readme",
  "dependencies": {
    "@babel/cli": "^7.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.0",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-react": "^7.10.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^7.1.5",
    "npm-run-all": "^4.1.5",
    "react4xp-build-components": "^0.7.7",
    "react4xp-buildconstants": "^0.8.4",
    "react4xp-runtime-externals": "^0.3.5",
    "react4xp-runtime-nashornpolyfills": "^0.3.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/proposal-object-rest-spread"
    ]
  }
}
